{
    "version": 2,
    "builds": [
        {
            "src": "sentiment_analysis/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize":"50mb","runtime":"python3.9"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }

        }
    ],
    "routes": [
      
      {
        "src": "/(.*)",
        "dest": "sentiment_analysis/wsgi.py"
      }
    ]
  }
  